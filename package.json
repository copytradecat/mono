{
  "name": "copy-trade-cat",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"next dev\" \"npm run bot:dev\"",
    "build": "next build",
    "start": "concurrently \"next start\" \"npm run bot:start\"",
    "lint": "next lint",
    "bot:dev": "nodemon --watch src/discord --ext ts,json --exec ts-node --esm src/discord/bot.ts",
    "bot:start": "ts-node-esm src/discord/bot.ts"
  },
  "dependencies": {
    "@auth/core": "^0.35.0",
    "@jup-ag/wallet-adapter": "1.0.0-alpha-5",
    "@solana/wallet-adapter-base": "^0.9.23",
    "@solana/wallet-adapter-react": "^0.15.35",
    "@solana/wallet-adapter-react-ui": "^0.9.35",
    "@solana/wallet-adapter-wallets": "^0.19.32",
    "@solana/web3.js": "^1.95.3",
    "crypto-js": "^4.2.0",
    "discord.js": "^14.16.2",
    "dotenv": "^16.4.5",
    "mongodb": "^6.9.0",
    "mongoose": "^8.6.2",
    "next": "^14.2.11",
    "next-auth": "^4.24.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sonner": "^1.5.0",
    "styled-components": "^6.1.13",
    "twin.macro": "^3.4.1"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.21",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^20.16.5",
    "@types/react": "^18.3.6",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.20",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-styled-components": "^2.1.4",
    "concurrently": "^9.0.1",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.11",
    "nodemon": "^3.1.4",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.11",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  }
}
