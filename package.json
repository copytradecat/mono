{
  "name": "copy-trade-cat",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"next dev\" \"npm run bot:dev\" \"npm run server:dev\"",
    "start": "concurrently \"next start\" \"npm run bot:start\" \"npm run server:start\"",
    "lint": "next lint",
    "ui:build": "next build",
    "ui:dev": "next dev",
    "ui:start": "next start",
    "bot:dev": "nodemon --watch src/discord --ext ts,json --exec npx tsx src/discord/bot.ts",
    "bot:start": "npx tsx src/discord/bot.ts",
    "bot:test": "npx tsx src/discord/test-bot.ts",
    "server:dev": "nodemon --watch signing-service --ext ts,json --exec npx tsx signing-service/server.ts",
    "server:start": "npx tsx signing-service/server.ts"
  },
  "dependencies": {
    "@auth/core": "^0.35.3",
    "@jup-ag/api": "^6.0.27",
    "@jup-ag/core": "4.0.0-beta.21",
    "@jup-ag/wallet-adapter": "1.0.0-alpha-5",
    "@metaplex-foundation/js": "^0.20.1",
    "@metaplex-foundation/mpl-token-metadata": "^3.2.1",
    "@project-serum/anchor": "^0.26.0",
    "@solana/spl-token": "^0.4.8",
    "@solana/wallet-adapter-base": "^0.9.23",
    "@solana/wallet-adapter-react": "^0.15.35",
    "@solana/wallet-adapter-react-ui": "^0.9.35",
    "@solana/wallet-adapter-wallets": "^0.19.32",
    "@solana/web3.js": "^1.95.3",
    "axios": "^1.7.7",
    "bigint-buffer": "^1.1.5",
    "bs58": "^6.0.0",
    "cross-fetch": "^4.0.0",
    "crypto-js": "^4.2.0",
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "limiter": "^2.1.0",
    "mongodb": "^6.9.0",
    "mongoose": "^8.7.0",
    "next": "^14.2.13",
    "next-auth": "^4.24.8",
    "node-fetch": "^3.3.2",
    "node-polyfill-webpack-plugin": "^4.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rpc-websockets": "7.5.1",
    "sonner": "^1.5.0",
    "styled-components": "^6.1.13",
    "twin.macro": "^3.4.1"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.21",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^20.16.10",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.20",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-styled-components": "^2.1.4",
    "concurrently": "^9.0.1",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.11",
    "nodemon": "^3.1.7",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.13",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  }
}
